<script type="text/javascript">

function postform(){
	$.ajax({
    	data:$("#join_form").serialize(),
    	url:"/join/joinus",
    	dataType:"json",
    	type:"post",
    	success:function(data){
    		jsApiCall(data.data);
    	}
    });
}

jQuery.validator.addMethod("isMobile", function(value, element) {  
	 var length = value.length;  
	 var mobile = /^(13[0-9]{9})|(18[0-9]{9})|(14[0-9]{9})|(17[0-9]{9})|(15[0-9]{9})$/;  
	 return this.optional(element) || (length == 11 && mobile.test(value));  
	}, "请正确填写手机号码");  

$().ready(function() {
	 $("#zhifu").click(function(){
		 
		 /*  layer.open({
 		   type: 1
 		    ,content: '可传入任何内容，支持html。一般用于手机页面中'
 		    ,anim: 'up'
 		    ,style: 'position:fixed; bottom:0; left:0; width: 100%; padding:10px 0; border:none;'
 		   ,content: $('#confirmdiv').html()
 		 });  */
		 
		$("#join_form").submit();
		
	});
	
	$(".taocantr").click(function(){
		var tc = $(this).find("input[name=taocan]");
		
		var v= $(tc).val();
		
		$("input[name=taocan]").each(function(){
			if($(this).val()!=v){
				if($(this).is(":checked"))
				$(this).removeProp("checked",false);
			}
			else{
				if(!$(this).is(":checked"))
					$(this).prop("checked",true);
			}
		});
		return true;
	});
	
	// 在键盘按下并释放及提交后验证提交表单
	  $("#join_form").validate({
	    rules: {
	      name: "required",
	      address: "required",
	      phone: {
	        required: true,
	        isMobile:true,
	      },
	      recommend:{
	    	  isMobile:true,
	      },
	    },
	    messages: {
	    	name: "请输入您的姓名",
	        address: "请输入详细地址",
	        phone: {
	          required: "请输入您的手机号码",
	          isMobile: "请输入正确的手机号码",
	        },
	        recommend:{
	    	  isMobile:"请输入正确的推荐人手机号",
	        }
	       },
	       submitHandler:function(form){
	    	   $("#cname").html($("#name").val());
	    	   $("#cphone").html($("#phone").val());
	    	   $("#caddress").html($("#address").val());
	    	   $("#crecommend").html($("#recommend").val());
	    	   $("#carea").html($("#area").val());
	    	   $("#ctaocan").html($("input[name=taocan]:checked").attr("data"));
	    	   
	    	   layer.open({
	    		   type: 1
	    		    ,content: '可传入任何内容，支持html。一般用于手机页面中'
	    		    ,anim: 'up'
	    		    ,style: 'position:fixed; bottom:0; left:0; width: 100%; padding:10px 0; border:none;'
	    		   ,content: $('#confirmdiv').html()
	    		 });
	        }    
	    })
	});
function jsApiCall(data){
	WeixinJSBridge.invoke(
			"getBrandWCPayRequest",
			data.api,
			function(res){
				if(res.err_msg == "get_brand_wcpay_request:ok"){
					location.href=window.location.protocol+"//"+window.location.host +"/join/done?status=1&id="+data.join.id;
				}else{
					location.href=window.location.protocol+"//"+window.location.host +"/join/done?status=0&id="+data.join.id;
				}
			})
		};
function callpay(data){
	if (typeof WeixinJSBridge == "undefined"){
		if( document.addEventListener ){
			document.addEventListener("WeixinJSBridgeReady", jsApiCall, false);
		}else if (document.attachEvent){
			document.attachEvent("WeixinJSBridgeReady", jsApiCall);
			document.attachEvent("onWeixinJSBridgeReady", jsApiCall);
		}
	}else{
			jsApiCall(data);
	}
}
</script>


